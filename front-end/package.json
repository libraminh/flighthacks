  "scripts": {
    "dev": "cross-env NODE_ENV=development && webpack-dev-server --progress --config build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production && webpack --config build/webpack.config.prod.js",
    "build:static": "cross-env NODE_ENV=development && webpack --config build/webpack.config.static.js",
    "lint": "eslint --ext .js,.vue src --fix",
    "lint:css": "stylelint assets/css/**/**/*.scss --fix",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": [
    "defaults",
    "ie 10"
  ],
  "stylelint": {
